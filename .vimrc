" Turn on syntax highlighting"
syntax on

" Show line numbers"
set number

" Use tabs to indent"
set noet ci pi sts=0 sw=4 ts=4 
"set noexpandtab copyindent preserveindent softtabstop=0 shiftwidth=4 tabstop=4"

" Auto indent"
set autoindent

" Color schemes"
set t_Co=256
set background=dark
let g:solarized_termcolors=256
let g:solarized_termtrans=1
